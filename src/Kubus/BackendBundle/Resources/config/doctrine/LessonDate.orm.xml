<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Kubus\BackendBundle\Entity\LessonDateRepository" name="Kubus\BackendBundle\Entity\LessonDate">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="lessonId" type="integer" column="lesson_id"/>
    <field name="locationId" type="integer" column="location_id"/>
    <field name="beginAt" type="datetime" column="begin_at"/>
    <field name="endAt" type="datetime" column="end_at"/>
    <field name="createdAt" type="datetime" column="created_at"/>
    <field name="updatedAt" type="datetime" column="updated_at"/>
  </entity>
</doctrine-mapping>
