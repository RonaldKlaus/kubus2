<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Kubus\BackendBundle\Entity\ApplicationRepository" name="Kubus\BackendBundle\Entity\Application">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="personId" type="integer" column="person_id"/>
    <field name="lessonId" type="integer" column="lesson_id"/>
    <field name="stateId" type="integer" column="state_id"/>
    <field name="invitedAt" type="datetime" column="invited_at"/>
    <field name="certifiedAt" type="datetime" column="certified_at"/>
    <field name="paid" type="boolean" column="paid"/>
    <field name="invoice" type="string" column="invoice" length="255"/>
    <field name="invoiceSend" type="boolean" column="invoice_send"/>
    <field name="credits" type="string" column="credits" length="255"/>
    <field name="mark" type="float" column="mark"/>
    <field name="childCare" type="boolean" column="child_care"/>
    <field name="liableToPay" type="boolean" column="liable_to_pay"/>
    <field name="createdAt" type="datetime" column="created_at"/>
    <field name="updatedAt" type="datetime" column="updated_at"/>
  </entity>
</doctrine-mapping>
