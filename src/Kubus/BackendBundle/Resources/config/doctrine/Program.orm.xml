<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="Kubus\BackendBundle\Entity\ProgramRepository" name="Kubus\BackendBundle\Entity\Program">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="title" type="string" column="title" length="255"/>
    <field name="description" type="string" column="description" length="255"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="domainId" type="integer" column="domain_id"/>
  </entity>
</doctrine-mapping>
