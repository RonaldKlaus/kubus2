<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Kubus\BackendBundle\Entity\Teacher" table="teacher">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="personId" type="integer" column="person_id" nullable="false"/>
    <field name="taxNumber" type="string" column="tax_number" length="255" nullable="false"/>
    <field name="financeOffice" type="string" column="finance_office" length="255" nullable="false"/>
    <field name="accountNumber" type="string" column="account_number" length="255" nullable="false"/>
    <field name="bankCode" type="string" column="bank_code" length="255" nullable="false"/>
    <field name="bankName" type="string" column="bank_name" length="255" nullable="false"/>
    <field name="firm" type="string" column="firm" length="255" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
  </entity>
</doctrine-mapping>
