{% extends 'KubusBackendBundle::layout.html.twig' %}

{% block content -%}
    <h1>Application</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Personid</th>
                <td>{{ entity.personId }}</td>
            </tr>
            <tr>
                <th>Lessonid</th>
                <td>{{ entity.lessonId }}</td>
            </tr>
            <tr>
                <th>Stateid</th>
                <td>{{ entity.stateId }}</td>
            </tr>
            <tr>
                <th>Invitedat</th>
                <td>{{ entity.invitedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Certifiedat</th>
                <td>{{ entity.certifiedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Paid</th>
                <td>{{ entity.paid }}</td>
            </tr>
            <tr>
                <th>Invoice</th>
                <td>{{ entity.invoice }}</td>
            </tr>
            <tr>
                <th>Invoicesend</th>
                <td>{{ entity.invoiceSend }}</td>
            </tr>
            <tr>
                <th>Credits</th>
                <td>{{ entity.credits }}</td>
            </tr>
            <tr>
                <th>Mark</th>
                <td>{{ entity.mark }}</td>
            </tr>
            <tr>
                <th>Childcare</th>
                <td>{{ entity.childCare }}</td>
            </tr>
            <tr>
                <th>Liabletopay</th>
                <td>{{ entity.liableToPay }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('application') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('application_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
