{% extends 'KubusBackendBundle::layout.html.twig' %}

{% block content -%}
    <h1>Lesson</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Courseid</th>
                <td>{{ entity.courseId }}</td>
            </tr>
            <tr>
                <th>Stateid</th>
                <td>{{ entity.stateId }}</td>
            </tr>
            <tr>
                <th>Beginat</th>
                <td>{{ entity.beginAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Endat</th>
                <td>{{ entity.endAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Numberparticipantmin</th>
                <td>{{ entity.numberParticipantMin }}</td>
            </tr>
            <tr>
                <th>Participantsminnumber</th>
                <td>{{ entity.participantsMinNumber }}</td>
            </tr>
            <tr>
                <th>Participantsmaxnumber</th>
                <td>{{ entity.participantsMaxNumber }}</td>
            </tr>
            <tr>
                <th>Childcare</th>
                <td>{{ entity.childCare }}</td>
            </tr>
            <tr>
                <th>Publishat</th>
                <td>{{ entity.publishAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Charge</th>
                <td>{{ entity.charge }}</td>
            </tr>
            <tr>
                <th>Externurl</th>
                <td>{{ entity.externUrl }}</td>
            </tr>
            <tr>
                <th>Graduationyear</th>
                <td>{{ entity.graduationYear }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ entity.updatedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('lesson') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('lesson_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
